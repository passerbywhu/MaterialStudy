<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.passerbywhu.materialstudy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/ActionBarTheme.WithoutActionBar">
        <activity android:name=".MainActivity">

        </activity>
        <activity android:name=".behavior.fab.FABActivity">
        </activity>
        <activity android:name=".NestedScrollSwipeRefreshActivity">
        </activity>
        <activity android:name=".behavior.footer.FooterActivity">
        </activity>
        <activity android:name=".behavior.follow.FollowActivity">
        </activity>
        <activity android:name=".behavior.scrollToTop.ScrollToTopActivity">
        </activity>
        <activity android:name=".nestedscroll.NestedScrollingActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>
